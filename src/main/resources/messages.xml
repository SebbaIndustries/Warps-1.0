<messages>

    <!--
    [!] AND symbols (&) are not allowed so use &amp; or § [!]
    List of Color Codes
    &0 -> &amp;0 <-> §0 (Black)
    &1 -> &amp;1 <-> §1 (Dark Blue)
    &2 -> &amp;2 <-> §2 (Dark Green)
    &3 -> &amp;3 <-> §3 (Dark Aqua)
    &4 -> &amp;4 <-> §4 (Dark Red)
    &5 -> &amp;5 <-> §5 (Dark Purple)
    &6 -> &amp;6 <-> §6 (Gold)
    &7 -> &amp;7 <-> §7 (Gray)
    &8 -> &amp;8 <-> §8 (Dark Gray)
    &9 -> &amp;9 <-> §9 (Blue)
    &a -> &amp;a <-> §a (Green)
    &b -> &amp;b <-> §b (Aqua)
    &c -> &amp;c <-> §c (Red)
    &d -> &amp;d <-> §d (Light Purple)
    &e -> &amp;e <-> §e (Yellow)
    &f -> &amp;f <-> §f (White)
    List of Format Codes
    &k -> &amp;k <-> §k (Obfuscated)
    &l -> &amp;l <-> §l (Bold)
    &m -> &amp;m <-> §m (Strikethrough)
    &n -> &amp;n <-> §n (Underline)
    &o -> &amp;o <-> §o (Italic)
    &r -> &amp;r <-> §r (Reset)
    [!] AND symbols (&) are not allowed so use &amp; or § [!]
    -->

    <!-- Prefix for messages -->
    <prefix>
        <msg lang="EN">§8[§dWarps§8]§r</msg>
        <msg lang="SI">§8[§dWarpi§8]§r</msg>
    </prefix>

    <!-- No permission message -->
    <message id="0">
        <msg lang="EN" prefix="true">§7You do not have permission to execute this command!</msg>
        <msg lang="SI" prefix="true">§7Nimas dovoljenja za uporabo te komande!</msg>
    </message>

    <!-- Already existing warp message -->
    <message id="1">
        <msg lang="EN" prefix="true">§7Failed to create specified warp! ({warp-name}, {reason})</msg>
    </message>

    <!-- Blacklisted Warp name message -->
    <message id="2">
        <msg lang="EN" prefix="true">§cFailed to create warp as it contains a blacklisted word! ({warp-name}</msg>
    </message>

    <!-- Successfully created warp message -->
    <message id="3">
        <msg lang="EN" prefix="true">§aSuccessfully created warp ({warp-name}, {warp-location}, {warp-world})</msg>
    </message>

    <!-- Invalid warp location -->
    <message id="4">
        <msg lang="EN" prefix="true">Failed to create warp, due to invalid location</msg>
    </message>

    <!-- Invalid warp message -->
    <message id="5">
        <msg lang="EN" prefix="true">Invalid warp!</msg>
    </message>

    <!-- Not warp owner message -->
    <message id="6">
        <msg lang="EN" prefix="true">You do not own this warp! ({warp-owner}</msg>
    </message>

    <!-- Error when removing a warp message -->
    <message id="7">
        <msg lang="EN" prefix="true">Failed to remove warp! (Contact admin)</msg>
    </message>

    <!-- Successfully deleted warp message -->
    <message id="8">
        <msg lang="EN" prefix="true">Deleted warp ({warp-name}, {warp-location})</msg>
    </message>

    <!-- Successfully changed warps location message -->
    <message id="9">
        <msg lang="EN" prefix="true">You have successfully changed this warps location! (New {warp-location})</msg>
    </message>

    <!-- Invalid command argument message -->
    <message id="10">
        <msg lang="EN" prefix="true">Invalid command argument!</msg>
    </message>

    <!-- Invalid player message -->
    <message id="11">
        <msg lang="EN" prefix="true">Invalid player</msg>
    </message>

    <!-- Changed warp owner message -->
    <message id="12">
        <msg lang="EN" prefix="true">Successfully changed warp owner! (Old: {warp-previous-owner}, New: {warp-new-owner}</msg>
    </message>

    <!-- New owner cannot be previous owner message -->
    <message id="13">
        <msg lang="EN" prefix="true">You cannot give yourself owner again Bimbo..</msg>
    </message>

    <!-- Changed warp status -->
    <message id="14">
        <msg lang="EN" prefix="true">Changed {warp-name} warp status to {warp-status}</msg>
    </message>

    <!-- Private warp -->
    <message id="15">
        <msg lang="EN" prefix="true">Cannot teleport to specified warp as it is private! Ask {warp-owner} to make it public</msg>
    </message>

    <!-- Unsafe location teleport message -->
    <message id="16">
        <msg lang="EN" prefix="true">Cannot teleport as the warps location is unsafe! Contact {warp-owner} and tell me to jump off a cliff</msg>
    </message>

    <!-- Warp teleportation started message -->
    <message id="17">
        <msg lang="EN" prefix="true">Starting warp teleportation!!!!!!!</msg>
    </message>

    <!-- Cannot rate a private warp message -->
    <message id="18">
        <msg lang="EN" prefix="true">Unable to rate a private warp!</msg>
    </message>

    <!-- Successfully rates a warp message -->
    <message id="19">
        <msg lang="EN" prefix="true">You have rated {warp-owner}'s warp ({warp-name}) with {rating}</msg>
    </message>

    <!-- Unable to rate your own warp message -->
    <message id="20">
        <msg lang="EN" prefix="true">You can't rate your own warp..</msg>
    </message>

    <!-- Too long warp description -->
    <message id="21">
        <msg lang="EN" prefix="true">Inputted description is too long.</msg>
    </message>

    <!-- Successfully set warps description -->
    <message id="22">
        <msg lang="EN" prefix="true">Successfully set warps description to: {warp-description}</msg>
    </message>

    <!-- Removed warps description -->
    <message id="23">
        <msg lang="EN" prefix="true">Successfully removed {warp-name} Warp's description!</msg>
    </message>

    <!-- No warps message -->
    <message id="24">
        <msg lang="EN" prefix="true">There is no warps created!</msg>
    </message>

    <!-- Listed warps message -->
    <message id="25">
        <msg lang="EN" prefix="false">§8[§b{target-name}'s Warps§8] §f{warp-names}</msg>
    </message>

    <!-- Target has no warps message -->
    <message id="26">
        <msg lang="EN" prefix="true">Target does not have any warps!</msg>
    </message>
</messages>