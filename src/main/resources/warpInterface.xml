<?xml version="1.0" encoding="utf-8" ?>
<interface>
    <pages rows="5" display="§8{type} Warps (§2{warp-amount}§8)">

        <warps slots="10,11,12,13,14,15,16,19,20,21,22,23,24,25,28,29,30,31,32,33,34">
            <material>DIAMOND</material>
            <display>§2Warp {warp-name}</display>
            <lore>
  §8* §bOwner§8: §f{warp-owner}
  §8* §bStatus§8: {warp-status}
  §8* §bRating§8: §f{warp-rating}
  §8* §bLocation§8: §f{warp-location}
  §8* §bWorld§8: §f{warp-world}

  §8* §bDescription§8: §7{warp-description}

  §fClick to teleport to this warp.
            </lore>
        </warps>

        <button action="next-page" slot="41">
            <material>SPECTRAL_ARROW</material>
            <display>§8Next Page</display>
            <lore>$empty</lore>
        </button>

        <button action="previous-page" slot="39">
            <material>SPECTRAL_ARROW</material>
            <display>§8Next Page</display>
            <lore>$empty</lore>
        </button>

        <button action="none" slot="40">
            <material>KNOWLEDGE_BOOK</material>
            <display>§8Some info tralala</display>
            <lore>info.</lore>
        </button>

        <background material="GRAY_STAINED_GLASS_PANE"/>

    </pages>
    <selector rows="3" display="Warp Selector Menu">

        <button type="server" slot="">
            <material>DIAMOND_BLOCK</material>
            <display>§9Server Warps</display>
            <lore>$empty</lore>
        </button>
        <button type="player" slot="">
            <material>GOLD_BLOCK</material>
            <display>§2Player Warps</display>
            <lore>$empty</lore>
        </button>

        <background material="GRAY_STAINED_GLASS_PANE"/>

    </selector>
</interface>